{
  "extends": ["plugin:prettier/recommended", "plugin:react/recommended"],
  "env": {
    "mocha": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "rules": {
    "react/prop-types": "off"
  },
  "settings": {
    "react": {
      "version": "15.0"
    }
  }
}
